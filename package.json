{
  "name": "vue-addr-picker",
  "version": "1.0.5",
  "description": "A Vue.js project",
  "author": "Fery Pan <panfeng1991@gmail.com>",
  "main": "dist/vue-addr-picker.umd.js",
  "module": "dist/vue-addr-picker.esm.js",
  "unpkg": "dist/vue-addr-picker.min.js",
  "scripts": {
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-addr-picker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-addr-picker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-addr-picker.min.js",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg"
  },
  "dependencies": {
    "mint-ui": "^2.2.13"
  },
  "devDependencies": {
    "rollup": "^0.66.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/socalledPF/vue-addr-picker"
  },
  "homepage": "https://53m085z024.codesandbox.io/#/"
}
